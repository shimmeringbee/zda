{
  "Name": "zcl",
  "Rules": [
    {
      "Filter": "(0x0001 in Endpoint[Self].InClusters)",
      "Actions": {
        "Capabilities": {
          "Add": {
            "ZCLPowerSupply": {
              "ZigbeeClusterID": "Fn.ClusterID(0x0001)",
              "ZigbeeEndpoint": "Fn.Endpoint(Self)"
            }
          }
        }
      }
    },
    {
